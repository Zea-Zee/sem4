        "# Функция для прогона одного эпизода с заданной стратегией\n",
        "def run_episode(policy, env):\n",
        "    state = env.reset()\n",
        "    is_done = False\n",
        "    total_reward = 0\n",
        "    while not is_done:\n",
        "        env.render()\n",
        "        action = policy.make_action(state)\n",
        "        state, reward, is_done, _ = env.step(action)\n",
        "        total_reward += reward\n",
        "    env.close()\n",
        "    return total_reward"
      ]
    },
